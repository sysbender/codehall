[
  {
    "name": "starter",
    "url": "https://livecodes.io/?template=tailwindcss",
    "config": {
      "markup": {
        "language": "html",
        "content": "app.html"
      },
      "style": {
        "language": "css",
        "content": "style.css"
      },
      "script": {
        "language": "javascript",
        "content": "script.js"
      },
      "processors": ["tailwindcss"],
      "customSettings": {
        "tailwindcss": {
          "plugins": [
            "@tailwindcss/forms",
            "@tailwindcss/typography",
            "@tailwindcss/aspect-ratio",
            "@tailwindcss/line-clamp"
          ],
          "theme": {
            "extend": {
              "colors": {
                "sky": {
                  "50": "#f0f9ff",
                  "100": "#e0f2fe",
                  "200": "#bae6fd",
                  "300": "#7dd3fc",
                  "400": "#38bdf8",
                  "500": "#0ea5e9",
                  "600": "#0284c7",
                  "700": "#0369a1",
                  "800": "#075985",
                  "900": "#0c4a6e"
                },
                "cyan": {
                  "50": "#ecfeff",
                  "100": "#cffafe",
                  "200": "#a5f3fc",
                  "300": "#67e8f9",
                  "400": "#22d3ee",
                  "500": "#06b6d4",
                  "600": "#0891b2",
                  "700": "#0e7490",
                  "800": "#155e75",
                  "900": "#164e63"
                }
              }
            }
          }
        }
      }
    }
  }
]
